<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.noteversion1"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
     <uses-permission android:name="android.permission.CAMERA"/>
    

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light" >
        <activity
            android:name="com.example.noteversion1.activities.RegisterActivity"
            android:label="@string/title_activity_register" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/title_activity_list_time" >
        </activity>
        <activity
            android:name=".activities.SetListNameActivity"
            android:label="@string/title_activity_create_list_screen" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.noteversion1.MainActivity" />
        </activity>
        <activity
            android:name=".activities.ListTimeActivity"
            android:label="@string/title_activity_list_time" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.noteversion1.SelectUsersActivity" />
        </activity>
        <activity
            android:name=".activities.SelectUsersActivity"
            android:label="@string/title_activity_select_users" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.noteversion1.CreateListActivity" />
        </activity>
        <activity
            android:name=".activities.ListSettingActivity"
            android:label="@string/title_activity_list_setting" >
        </activity>
        <activity
            android:name=".activities.ShoppingListActivity"
            android:label="@string/title_activity_shopping_list" >
        </activity>
        
    </application>

</manifest>